{
    "manifest_version": 3,
    "name": "Focus",
    "version": "1.1",
    "description": "blocks sites so you can lock in",
    "icons": {
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    }
}